<!DOCTYPE html>
<html lang="en">
  <head>
    @RenderSection("gce", required: false)
    <meta charset="utf-8">
    <title>@ViewBag.Title - I love eco hotels</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    @{
        Html.RequiresCss("/assets/bootstrap/css/bootstrap.css", 0);
        //Html.RequiresCss("/assets/bootstrap/css/bootstrap-responsive.css", 1);

        //Html.RequiresCss("/assets/3rdparty/galleria/themes/classic/galleria.classic.css", 10);
        
        Html.RequiresCss("/assets/css/site.css", 100);
    }
    @MvcHtmlString.Create(Html.RenderCssHere())

    @* Le HTML5 shim, for IE6-8 support of HTML5 elements *@
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    @RenderSection("stylesheet", required: false)

</head>
<body>
    <div id="contactable"><!-- contactable html placeholder --></div>

    @Html.Partial("_MenuPartial", new { SelectedItem = @ViewBag.MenuItem })

    @RenderBody()


    <script src="//code.jquery.com/jquery-1.8.1.min.js" type="text/javascript"></script>
    <script type="text/javascript" >window.jQuery || document.write("<script src='js/libs/jquery-1.5.1.min.js'>\x3C/script>")</script>
    <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js" type="text/javascript"></script>
    <script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.9/jquery.validate.min.js" type="text/javascript"></script>

    @{
        Html.RequiresJs("/assets/js/jquery.form.min.js", 10);
        Html.RequiresJs("/assets/js/jquery.contactable.min.js", 11);
        Html.RequiresJs("/assets/bootstrap/js/bootstrap.min.js", 20);
        
        //Html.RequiresJs("/assets/3rdparty/galleria/galleria-1.2.7.min.js", 30);
        //Html.RequiresJs("/assets/3rdparty/galleria/themes/classic/galleria.classic.js", 31);
        
        Html.RequiresJs("/assets/js/site.js", 100);
    }
        
    @RenderSection("javascript", required: false)

    @MvcHtmlString.Create(Html.RenderJsHere())
</body>
</html>