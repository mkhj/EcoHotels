<!doctype html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" >
    <title>@ViewBag.Title</title>
    @{
        Html.RequiresCss("/assets/bootstrap-2.1.0/css/bootstrap.min.css", 0);
        //Html.RequiresCss("/assets/bootstrap/css/bootstrap-responsive.css", 1);
        Html.RequiresCss("/assets/css/app.css", 100);
    }
    @MvcHtmlString.Create(Html.RenderCssHere())
       	
    @RenderSection("stylesheet", required: false)

    @RenderSection("javascript", required: false)
</head>
<body>
    @RenderBody()

    @{
        Html.RequiresJs("/assets/3rdparty/jQuery/jquery-1.8.1.min.js", 1);
        Html.RequiresJs("/assets/3rdparty/jQuery/jquery-ui-1.8.23.custom.min.js", 2);
        Html.RequiresJs("/assets/3rdparty/jQuery/jquery.validate.min.js", 3);

        //Html.RequiresJs("/assets/js/jquery.form.min.js", 10);
        Html.RequiresJs("/assets/js/jquery.contactable.min.js", 11);
        //Html.RequiresJs("/assets/bootstrap-2.1.0/js/bootstrap.min.js", 20);        
        
        Html.RequiresJs("/assets/js/jquery.form.min.js", 10);
        Html.RequiresJs("/assets/bootstrap-2.1.0/js/bootstrap.min.js", 20);

        // The Templates plugin is included to render the upload/download listings
        Html.RequiresJs("/assets/js/fileupload/tmpl.min.js", 30);
        // The Load Image plugin is included for the preview images and image resizing functionality
        Html.RequiresJs("/assets/js/fileupload/load-image.min.js", 31);
        // The Canvas to Blob plugin is included for image resizing functionality
        Html.RequiresJs("/assets/js/fileupload/canvas-to-blob.min.js", 32);

        Html.RequiresJs("/assets/js/fileupload/jquery.fileupload.js", 33);
        Html.RequiresJs("/assets/js/fileupload/jquery.fileupload-fp.js", 34);
        Html.RequiresJs("/assets/js/fileupload/jquery.fileupload-ui.js", 35);

        Html.RequiresJs("/assets/js/app.js", 100);
        Html.RequiresJs("/assets/js/app.views.admin.js", 101);
    }
    @MvcHtmlString.Create(Html.RenderJsHere())
</body>
</html>